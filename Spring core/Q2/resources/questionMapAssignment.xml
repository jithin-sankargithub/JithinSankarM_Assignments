<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:context="http://www.springframework.org/schema/context"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans.xsd
    http://www.springframework.org/schema/context
    http://www.springframework.org/schema/context/spring-context.xsd">
    
           

 

  <bean id="question" class="com.springcoreassignment.q2.QuestionMap" >
  <property name="questionId" value="1"></property>
  <property name="question" value="Best programming languages"></property>
  <property name="answers">
  <map>
    <entry key="1" value ="Java"/>
    <entry key="2" value ="C++"/>
    <entry key="3" value ="Python"/>
  </map>
  </property>
  
  </bean>


 

</beans>